using System;
					
public class Program
{
	public static void Main()
	{
		int length = 13; //pwedeng baguhin ung number
		int width = 6;
		int area = length * width;
		Console.WriteLine("Given the following rectangle measurements: ");
		Console.WriteLine("Length = "+ length);Console.WriteLine("Width = "+ width);
		Console.WriteLine("The area of the rectangle is: "+ area);
	}
    }
